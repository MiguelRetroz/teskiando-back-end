{
  "name": "teskiando-back-end",
  "version": "1.0.0",
  "description": "App for organizaing tasks - Back-end",
  "main": "src/api/server.js",
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelRetroz/teskiando-back-end.git"
  },
  "keywords": [
    "todo",
    "task",
    "tasks",
    "organisation"
  ],
  "author": "Miguel Retroz <contato.miguelretroz@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiguelRetroz/teskiando-back-end/issues"
  },
  "homepage": "https://github.com/MiguelRetroz/teskiando-back-end#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-trybe-backend": "^1.0.4",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-rescue": "^1.1.31",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.1"
  }
}
